.certificateLogin
  .photo
    width 100%
    background-size: cover;
    background-image url("../../../common/img/background.jpg")
    background-position top
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    .cntBox
      width: 100%
      min-height: 4.18rem
      background: transparent
      margin-bottom: 0.32rem

.container-fluid
  padding: 0
  min-height 100%
  .logo-container
    text-align: center
    margin-top: 0.32rem
    margin-bottom: 0.42rem
  .logo
    .logoSize
      width: 1rem
      height: 1rem

  .loginInfor
    width: 100%
    height: auto
    min-height: 100%
    padding: 0.64rem 0.15rem 0
    position: absolute
    display: flex
    display: -webkit-flex
    flex-direction: column
    justify-content space-between
    -webkit-justify-content: space-between
    align-items: center
    -webkit-align-items: center
    .cntBox
      width: 100%
    .form-group
      height: 0.7rem
      margin-bottom: 0.32rem
      .form-label
        margin-bottom: 0.18rem
        padding-left: 0.06rem
      .form-label-code
        margin-bottom: 0.05rem
        padding-left: 0.06rem
      .form-label-font
        font-family: SourceHanSansCN-Regular;
        font-size: 0.14rem
        color: #f2f2f2
      .form-control-container
        padding-left: 0.06rem
        width: 100%
      .form-control
        font-family: SourceHanSansCN-Regular;
        margin-bottom: 0.09rem
        border: none
        outline: medium
        width: 100%
        //height: 0.22rem
        height: 90%
        background-color: transparent
        font-size: 0.18rem
        color: #d5a764
      input
        &::-webkit-input-placeholder
          font-family: SourceHanSansCN-Regular;
          font-size 0.18rem
          color #7D8497
          letter-spacing 0
      .form-control-code
        border: none
        outline: medium
        width: 100%
        height: 0.22rem
        background-color: transparent
        font-size: 0.18rem
        color: #d5a764
    .loginDetail
      width: 100%
      .loginAgreement
        font-family: SourceHanSansCN-Regular
        font-size: 0.12rem
        color: #7D8497
        letter-spacing: 0
        line-height: 0.18rem
        padding: 0 0.11rem 0.12rem
        span
          color: #D5A764
      .btn-container
        .btn-success
          margin 0
      .register
        margin-top 0.3rem
        margin-bottom 0.3rem
    .icon-invisible
      position: absolute
      right: 0.2rem

    .icon-invisible:before
      font-size: 0.23rem

    .icon-visible:before
      font-size: 0.23rem

    .icon-visible
      position: absolute
      right: 0.2rem
    .register
      width: 100%
      margin-bottom 0.3rem

    .weui-cells
      /*background-color palegoldenrod !important*/
      background-color: transparent !important
      margin-top 0 !important
      position static !important
    .weui-select
      padding-left 0 !important
      font-size 0.18rem !important
      font-family: SourceHanSansCN-Regular !important
      color #7D8497 !important

    .weui-cell_select .weui-cell__bd:after {

      height: 0.1rem !important;
      width: 0.1rem !important;
      border-width: 0.02rem 0.02rem 0 0 !important;
      border-color: #d5a764 !important;

    }
    .weui-select {
      /*background-color  !important*/
    }

#showBank
  font-family: SourceHanSansCN-Regular
  height 0.38rem
  line-height 0.38rem
  font-size 0.18rem
  color #7D8497
  width: 100%
  display: inline-block

.ios-select-widget-box header.iosselect-header a.sure
  font-family SourceHanSansCN-Regular
  color #d7a853

.ios-select-widget-box header.iosselect-header a.close
  color #7d8497
  font-family SourceHanSansCN-Regular

.ios-select-widget-box header.iosselect-header h2
  font-family SourceHanSansCN-Regular

.iosselect-box
  font-family SourceHanSansCN-Regular




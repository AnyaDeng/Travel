    color: #b8b8b8
  &:-ms-input-placeholder
    color: #b8b8b8
  &::-moz-placeholder
    color: #b8b8b8
  &.errMsg
    &::-webkit-input-placeholder
      color: #e4393c
    &:-ms-input-placeholder
      color: #e4393c

input
  &.errMsg
    &::-moz-placeholder
      color: #e4393c

/*解决vue闪现*/
[v-cloak]
  display: none

.animated
  animation-duration: 0.3s
  animation-fill-mode: none

@-webkit-keyframes fadeIn {
  0% {
    opacity: 0
  }
  100% {
    opacity: 1
  }
}

@keyframes fadeIn {
  0% {
    opacity: 0
  }
  100% {

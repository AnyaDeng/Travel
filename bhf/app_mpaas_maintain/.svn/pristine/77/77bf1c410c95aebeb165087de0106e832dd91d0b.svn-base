//爱心鸿运来 心形跳动动画
@keyframes  donateRotate
{
  0% {
    transform translateY(0px)
  }

  25%{
    transform translateY(-2px)
  }

  50%{
    transform translateY(0px)
  }

  75%{
    transform translateY(-1px)
  }

  100% {
    transform translateY(0px)
  }

}



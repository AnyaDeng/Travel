/*头部组件*/
.header
  color: #fff
  width: 100%
  height: 0.64rem
  background: #263147
  padding: 0 0.2rem
  position: fixed
  top: 0
  left: 0
  display: flex
  display: -webkit-flex
  justify-content space-between
  -webkit-justify-content: space-between
  align-items: flex-end
  -webkit-align-items: flex-end
  .headerBox
    width: 100%
    height: 0.44rem
    display: flex
    display: -webkit-flex
    justify-content space-between
    -webkit-justify-content: space-between
    align-items: center
    -webkit-align-items: center
    [class*="icon-"]
      font-size: 0.23rem
      margin: 0
    .left-cnt
      [class*="icon-"]
        margin-right: 0.11rem
      a
        span
          font-family: SourceHanSansCN-Medium
      .headerModule
        display: flex
        display: -webkit-flex
        justify-content flex-start
        -webkit-justify-content: flex-start
        align-items: center
        -webkit-align-items: center
    .center-cnt
      font-size: 0.18rem
      text-align: center
      white-space: nowrap
      text-overflow: ellipsis
      overflow: hidden

    .right-cnt
      text-align: right
      color: #fff
      [class*="icon-"]
        margin-left: 0.11rem
      a
        color: #fff
        span
          font-family: SourceHanSansCN-Medium
      .headerModule
        display: flex
        display: -webkit-flex
        justify-content flex-end
        -webkit-justify-content: flex-end
        align-items: center
        -webkit-align-items: center
  .headerBoxWidth
    .left-cnt
      min-width: 25%
    .center-cnt
      width: 50%
    .right-cnt
      width: 25%
  .headerChangePercent
    .left-cnt
      min-width: 15%
    .center-cnt
      width: 70%
    .right-cnt
      width: 15%
  .headerBoxFlex
    width: 100%
    height: 0.44rem
    padding-bottom: 0.13rem
    display: flex
    display: -webkit-flex
    justify-content space-between
    -webkit-justify-content: space-between
    align-items: flex-end
    -webkit-align-items: flex-end

/*flex-布局*/
.flex-f
  display: flex
  display: -webkit-flex

.flex-f-i
  display: inline-flex
  display: -webkit-inline-flex

/*--主轴水平对齐*/

/*水平左对齐*/
.flex-x-vl
  justify-content: flex-start
  -webkit-justify-content: flex-start

/*水平居中*/
.flex-x-vc
  justify-content: center
  -webkit-justify-content: center

/*水平右对齐*/
.flex-x-vr
  justify-content flex-end
  -webkit-justify-content: flex-end

/*水平左右对齐*/
.flex-x-vsb
  justify-content space-between
  -webkit-justify-content: space-between

/*水平左右对齐（有间距）*/
.flex-x-vsr
  justify-content: space-around
  -webkit-justify-content: space-around

/*--垂直对齐*/
.flex-ht
  align-items: flex-start
  -webkit-align-items: flex-start

/*垂直居中*/
.flex-hc
  align-items: center
  -webkit-align-items: center

.flex-hb
  align-items: flex-end
  -webkit-align-items: flex-end

/*垂直等高*/
.flex-hs
  align-items: stretch
  -webkit-align-items: stretch

.flex-hl
  align-items: baseline
  -webkit-align-items: baseline

/*--侧轴水平对齐*/
.flex-y-vl
  align-content: flex-start
  -webkit-align-content: flex-start

.flex-y-vc
  align-content: center
  -webkit-align-content: center

.flex-y-vr
  align-content: flex-end
  -webkit-align-content: flex-end

.flex-y-vsb
  align-content: space-between
  -webkit-align-content: space-between

.flex-y-vsr
  align-content: space-around
  -webkit-align-content: space-around

/*--换行*/
.flex-ht-self
  align-self: flex-start

.flex-hc-self
  align-self: center

.flex-hb-self
  align-self: flex-end

.flex-noWrap
  flex-wrap: nowrap

.flex-wrap
  flex-wrap: wrap

.flex-wrapReverse
  flex-wrap: wrap-reverse

/*主轴方向*/
.flex-dir-r
  flex-direction: row

.flex-dir-rr
  flex-direction: row-reverse

.flex-dir-c
  flex-direction: column

.flex-dir-cr
  flex-direction: column-reverse

/*flex-grow*/
.flex-g1
  flex-grow: 1

.flex-g2
  flex-grow: 2

/*flex END*/
.mg_la
  margin-left: auto

/*全局公共样式 */
html,
body
  position: relative
  width: 100%
  height: 100%
  background-color: #fff

body
  color: #666
  font-size: 0.14rem
  font-family: -apple-system, system-ui, "PingFang SC", "PingFang TC", "PingFang HK", "Source Han Sans TC", "Source Han Sans SC", Roboto, Helvetica, "Helvetica Neue";
  padding-top: constant(safe-area-inset-top)
  padding-bottom: constant(safe-area-inset-bottom)

div
  box-sizing: border-box

input,
textarea,
button
  outline: none
  border: none
  box-shadow: none
  -webkit-box-shadow: none

ul
  list-style: none

table
  border-collapse: collapse
  border-spacing: 0

input

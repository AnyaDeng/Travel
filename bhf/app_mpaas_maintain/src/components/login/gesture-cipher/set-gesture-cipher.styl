.container-fluid
  padding: 0
  background: #fff

.setGestureCipher
  .setGestureContainer
    display: flex
    display: -webkit-flex
    flex-direction: column
    justify-content: center
    -webkit-justify-content: center
    align-items: center
    -webkit-align-items: center
    width: 100%
    position: absolute
    top: 0
    left: 50%
    -webkit-transform: translate(-50%, 0);
    transform: translate(-50%, 0);
    margin-top: 1.16rem
    .passInformation
      width: 100%
      //position: absolute
      //top: 0.64rem
      //left: 0
      display: flex
      justify-content: center
      align-items: center
      flex-direction: column
      div
        font-family: SourceHanSansCN-Regular
        letter-spacing: 0
        color: #232323
      div.welcoming
        width: 0.35rem
        height: 0.35rem
        margin: 0 0 0.2rem 0
        display: inline-flex
        display: -webkit-inline-flex
        flex-direction: column
        justify-content space-between
        -webkit-justify-content: space-between
        div
          display: inline-flex
          display: -webkit-inline-flex
          justify-content space-between
          -webkit-justify-content: space-between
          span
            display: inline-block
            width: 0.065rem
            height: 0.065rem
            background: #ccc
            border-radius: 50%
          span.successShow
            background: #d7ab69
          span.errorShow
            background: #F56F6E
      div.massage
        font-size: 0.14rem
      div.errorMessage
        color: #F56F6E

    .canvasStyle
      margin-top: 0.52rem
